        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.HelpMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.HelpSearchActivity" />
        <activity android:name=".activity.Privacysettings" />
        <activity android:name=".activity.HelpPrivaySercACtivaty" />
        <activity android:name=".activity.HelpPersonalCenter" />
        <activity android:name=".activity.HelpTaketaxi" />
        <activity android:name=".activity.Citypacler" />
        <activity android:name=".activity.HelpAddedAddress" />
        <activity android:name=".activity.HelpIntegralMall" />
